{
  "name": "emlearn",
  "version": "master",
  "description": "Enables MicroPython applications to run efficient Machine Learning models on microcontroller, without having to touch any C code",
  "author": "Jon Nordby",
  "license": "MIT",
  "homepage": "https://emlearn.github.io/emlearn-micropython",
  "arch": ["x64", "armv6m", "armv7m", "armv7emsp", "xtensawin"],
  "mpy":  ["6.3"],
  "urls": [
