    ["emlearn_iir_q15.mpy",       "{ARCH}_{MPY}/emlearn_iir_q15.mpy"],
    ["emlearn_fft.mpy",           "{ARCH}_{MPY}/emlearn_fft.mpy"],
    ["emlearn_iir.mpy",           "{ARCH}_{MPY}/emlearn_iir.mpy"],
    ["emlearn_kmeans.mpy",        "{ARCH}_{MPY}/emlearn_kmeans.mpy"],
    ["emlearn_neighbors.mpy",     "{ARCH}_{MPY}/emlearn_neighbors.mpy"],
    ["emlearn_trees.mpy",         "{ARCH}_{MPY}/emlearn_trees.mpy"],
    ["emlearn_cnn.mpy",           "{ARCH}_{MPY}/emlearn_cnn.mpy"]
  ]
}
